<Window
    x:Class="CourseRevit2025.FirstProject.View.SearchElementsWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:CourseRevit2025.FirstProject.View.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:CourseRevit2025.FirstProject.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="SearchElementsWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary Source="/CourseRevit2025.FirstProject;component/Resources/MainStyle.xaml" />
    </Window.Resources>
    <Grid>
        <controls:ShowElementsView Grid.Row="1" DataContext="{Binding}" />
    </Grid>
</Window>
